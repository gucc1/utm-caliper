{
  "caliper": {
    "blockchain": "fabric",
    "command": {
      "start": "docker-compose -f network/utm/docker-compose-e2e.yaml up -d;sleep 3s",
      "end": "docker-compose -f network/utm/docker-compose-e2e.yaml down;docker rm $(docker ps -aq);docker rmi $(docker images dev* -q)"
    }
  },
  "fabric": {
    "cryptodir": "network/utm/crypto-config",
    "network": {
      "orderer": {
        "url": "grpc://localhost:7050",
        "mspid": "OrdererMSP",
        "msp": "network/utm/crypto-config/ordererOrganizations/example.com/msp/",
        "server-hostname": "orderer.example.com",
        "tls_cacerts": "network/utm/crypto-config/ordererOrganizations/example.com/orderers/orderer.example.com/tls/ca.crt"
      },
      "org1": {
        "name": "peerOrg1",
        "mspid": "Org1MSP",
        "msp": "network/utm/crypto-config/peerOrganizations/org1.example.com/msp/",
        "ca": {
          "url": "http://localhost:7054",
          "name": "ca_peerOrg1"
        },
        "peer0": {
          "requests": "grpc://localhost:7151",
          "events": "grpc://localhost:7153",
          "server-hostname": "peer0.org1.example.com",
          "tls_cacerts": "network/utm/crypto-config/peerOrganizations/org1.example.com/peers/peer0.org1.example.com/tls/ca.crt"
        }
      },
      "org2": {
        "name": "peerOrg2",
        "mspid": "Org2MSP",
        "msp": "network/utm/crypto-config/peerOrganizations/org2.example.com/msp/",
        "ca": {
          "url": "http://localhost:7054",
          "name": "ca_peerOrg1"
        },
        "peer0": {
          "requests": "grpc://localhost:7251",
          "events": "grpc://localhost:7253",
          "server-hostname": "peer0.org2.example.com",
          "tls_cacerts": "network/utm/crypto-config/peerOrganizations/org2.example.com/peers/peer0.org2.example.com/tls/ca.crt"
        }
      }
    },
    "channel": [
      {
        "name": "mychannel",
        "deployed": false,
        "config": "network/utm/channel-artifacts/channel.tx",
        "organizations": ["org1", "org2"]
      }
    ],
    "chaincodes": [
      {
        "id": "utm",
        "path": "contract/fabric/utm",
        "language": "golang",
        "version": "v0",
        "channel": "mychannel"
      }
    ],
    "endorsement-policy": {
      "identities": [
        {
          "role": {
            "name": "member",
            "mspId": "Org1MSP"
          }
        },
        {
          "role": {
            "name": "member",
            "mspId": "Org2MSP"
          }
        }
      ],
      "policy": { "2-of": [{ "signed-by": 0 }, { "signed-by": 1 }] }
    },
    "context": {
      "invoke": "mychannel"
    }
  },
  "info": {
    "Version": "0.1",
    "Size": "2 Orgs with 1 Peers",
    "Orderer": "Solo",
    "Distribution": "Single Host"
  }
}
