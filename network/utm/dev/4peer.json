{
  "caliper": {
    "blockchain": "fabric",
    "command": {
      "start": "docker-compose -f network/utm/dev/docker-compose-4peer.yaml up -d;sleep 3s",
      "end": "docker-compose -f network/utm/dev/docker-compose-4peer.yaml down;docker rm $(docker ps -aq);docker rmi $(docker images dev* -q)"
    }
  },
  "fabric": {
    "cryptodir": "network/utm/config/crypto-config",
    "network": {
      "orderer": {
        "url": "grpc://localhost:7050",
        "mspid": "OrdererMSP",
        "msp": "network/utm/config/crypto-config/ordererOrganizations/example.com/msp/",
        "server-hostname": "orderer.example.com",
        "tls_cacerts": "network/utm/config/crypto-config/ordererOrganizations/example.com/orderers/orderer.example.com/tls/ca.crt"
      },
      "org1": {
        "name": "peerOrg1",
        "mspid": "Org1MSP",
        "msp": "network/utm/config/crypto-config/peerOrganizations/org1.example.com/msp/",
        "ca": {
          "url": "http://localhost:7154",
          "name": "ca.org1.example.com"
        },
        "peer1": {
          "requests": "grpc://localhost:7151",
          "events": "grpc://localhost:7153",
          "server-hostname": "peer0.org1.example.com",
          "tls_cacerts": "network/utm/config/crypto-config/peerOrganizations/org1.example.com/peers/peer0.org1.example.com/tls/ca.crt"
        }
      },
      "org2": {
        "name": "peerOrg2",
        "mspid": "Org2MSP",
        "msp": "network/utm/config/crypto-config/peerOrganizations/org2.example.com/msp/",
        "ca": {
          "url": "http://localhost:7154",
          "name": "ca.org1.example.com"
        },
        "peer1": {
          "requests": "grpc://localhost:7251",
          "events": "grpc://localhost:7253",
          "server-hostname": "peer0.org2.example.com",
          "tls_cacerts": "network/utm/config/crypto-config/peerOrganizations/org2.example.com/peers/peer0.org2.example.com/tls/ca.crt"
        }
      },
      "org3": {
        "name": "peerOrg3",
        "mspid": "Org3MSP",
        "msp": "network/utm/config/crypto-config/peerOrganizations/org3.example.com/msp/",
        "ca": {
          "url": "http://localhost:7154",
          "name": "ca.org1.example.com"
        },
        "peer1": {
          "requests": "grpc://localhost:7351",
          "events": "grpc://localhost:7353",
          "server-hostname": "peer0.org3.example.com",
          "tls_cacerts": "network/utm/config/crypto-config/peerOrganizations/org3.example.com/peers/peer0.org3.example.com/tls/ca.crt"
        }
      },
      "org4": {
        "name": "peerOrg4",
        "mspid": "Org4MSP",
        "msp": "network/utm/config/crypto-config/peerOrganizations/org4.example.com/msp/",
        "ca": {
          "url": "http://localhost:7154",
          "name": "ca.org1.example.com"
        },
        "peer1": {
          "requests": "grpc://localhost:7451",
          "events": "grpc://localhost:7453",
          "server-hostname": "peer0.org4.example.com",
          "tls_cacerts": "network/utm/config/crypto-config/peerOrganizations/org4.example.com/peers/peer0.org4.example.com/tls/ca.crt"
        }
      }
    },
    "channel": [
      {
        "name": "mychannel",
        "deployed": false,
        "config": "network/utm/config/mychannel.tx",
        "organizations": ["org1", "org2", "org3", "org4"]
      }
    ],
    "chaincodes": [
      {
        "id": "utm",
        "path": "contract/fabric/utm",
        "language": "golang",
        "version": "v0",
        "channel": "mychannel"
      }
    ],
    "endorsement-policy": {
      "identities": [
        {
          "role": {
            "name": "member",
            "mspId": "Org1MSP"
          }
        },
        {
          "role": {
            "name": "member",
            "mspId": "Org2MSP"
          }
        },
        {
          "role": {
            "name": "member",
            "mspId": "Org3MSP"
          }
        },
        {
          "role": {
            "name": "member",
            "mspId": "Org4MSP"
          }
        },
        {
          "role": {
            "name": "admin",
            "mspId": "Org1MSP"
          }
        }
      ],
      "policy": {
        "4-of": [
          { "signed-by": 0 },
          { "signed-by": 1 },
          { "signed-by": 2 },
          { "signed-by": 3 }
        ]
      }
    },
    "context": {
      "invoke": "mychannel"
    }
  },
  "info": {
    "Version": "0.1",
    "Size": "4 Org with 1 Peer",
    "Orderer": "Solo",
    "Distribution": "Single Host"
  }
}
