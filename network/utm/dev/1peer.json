{
  "caliper": {
    "blockchain": "fabric",
    "command": {
      "start": "docker-compose -f network/utm/dev/docker-compose-1peer.yaml up -d;sleep 3s",
      "end": "docker-compose -f network/utm/dev/docker-compose-1peer.yaml down;docker rm $(docker ps -aq);docker rmi $(docker images dev* -q)"
    }
  },
  "fabric": {
    "cryptodir": "network/utm/config/crypto-config",
    "network": {
      "orderer": {
        "url": "grpc://localhost:7050",
        "mspid": "OrdererMSP",
        "msp": "network/utm/config/crypto-config/ordererOrganizations/example.com/msp/",
        "server-hostname": "orderer.example.com",
        "tls_cacerts": "network/utm/config/crypto-config/ordererOrganizations/example.com/orderers/orderer.example.com/tls/ca.crt"
      },
      "org1": {
        "name": "peerOrg1",
        "mspid": "Org1MSP",
        "msp": "network/utm/config/crypto-config/peerOrganizations/org1.example.com/msp/",
        "ca": {
          "url": "http://localhost:7154",
          "name": "ca.org1.example.com"
        },
        "peer1": {
          "requests": "grpc://localhost:7151",
          "events": "grpc://localhost:7153",
          "server-hostname": "peer0.org1.example.com",
          "tls_cacerts": "network/utm/config/crypto-config/peerOrganizations/org1.example.com/peers/peer0.org1.example.com/tls/ca.crt"
        }
      }
    },
    "channel": [
      {
        "name": "mychannel",
        "deployed": false,
        "config": "network/utm/config/mychannel.tx",
        "organizations": ["org1"]
      }
    ],
    "chaincodes": [
      {
        "id": "utm",
        "path": "contract/fabric/utm",
        "language": "golang",
        "version": "v0",
        "channel": "mychannel"
      }
    ],
    "endorsement-policy": {
      "identities": [
        {
          "role": {
            "name": "member",
            "mspId": "Org1MSP"
          }
        },
        {
          "role": {
            "name": "admin",
            "mspId": "Org1MSP"
          }
        }
      ],
      "policy": {
        "1-of": [{ "signed-by": 0 }]
      }
    },
    "context": {
      "invoke": "mychannel"
    }
  },
  "info": {
    "Version": "0.1",
    "Size": "1 Org with 1 Peer",
    "Orderer": "Solo",
    "Distribution": "Single Host"
  }
}
